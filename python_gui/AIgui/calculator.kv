# Custom button
<CustButton@Button>:
	font_size: 32

<CalcGridLayout>:
	id: calculator
	display: op
	greet:o
	rows: 10
	padding: 10
	spacing: 10


	#BoxLayout:
		

	BoxLayout:
		CustButton:
			text: "7"
			on_press: calculator.callback(op.text)
	BoxLayout:
	BoxLayout:
		Label:
			id:o
			text:"dasfasd"
	BoxLayout:
		TextInput:
			id: op
			font_size: 32
			multiline: False

	BoxLayout:
		orientation:'vertical'
		CustButton:
			font_size: 20
			text: "query"
			on_press: calculator.callback(op.text)

